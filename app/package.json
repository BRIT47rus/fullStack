{
  "name": "@fullStack/app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "preview": "vite preview",
    "prettify": "prettier --loglevel warn --cache --write \"{src/**/*,*}.{ts,tsx,js,json,yml,scss}\" \"!**/{node_modules,dist}/**/*\"",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json"
  },
  "dependencies": {
    "@fullStack/backend": "workspace:*",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "formik": "^2.4.4",
    "formik-validator-zod": "^1.0.2",
    "include-media": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.15.0",
    "reset-css": "^5.0.2",
    "sass": "^1.85.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "globals": "^15.15.0",
    "prettier": "^3.0.1",
    "vite": "^6.2.0"
  }
}
